<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fh.shop.api.area.mapper.IAreaMapper" >

    <!-- 查询地区列表 -->
    <select id="queryAreaList" parameterType="java.lang.Integer" resultType="com.fh.shop.api.area.po.Area">
        select id,areaName,fid from t_area where fid = #{value}
    </select>

</mapper>